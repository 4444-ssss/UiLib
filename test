
local FGUI = game.Players.LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("GUI")
if FGUI then
    FGUI:Destroy()
end

local plrs = game:GetService'Players'
local plr = plrs.LocalPlayer
local LoopFlingV1 = {}
local LoopFlingV2 = {}

local function handleAntiRagdoll(state)
    local ragdoll = plr.Character:FindFirstChild("Ragdoll")

    if state then
        if ragdoll then
            ragdoll:Destroy()
        end
    else
        if not ragdoll then
            return
        end
    end
end

local function findplr(Target)
    if game:GetService'Players':FindFirstChild(Target) then
        return game:GetService'Players':FindFirstChild(Target)
    else
        if Target ~= nil and Target ~= "" and Target ~= " " and Target then
            local name = Target
            local found = false
            for _,v in pairs(game:GetService'Players':GetPlayers()) do 
                if not found and (v.Name:lower():sub(1,#name) == name:lower() or v.DisplayName:lower():sub(1,#name) == name:lower()) then
                    name = v
                    found = true
                end
            end
            if name ~= nil and name ~= Target then
                return name
            end
        end
    end
end

local function randomCFrame()
    return CFrame.new(math.random(-9e38, 9e38), -999, math.random(-9e38, 9e38))
end

