local x1 = Main:AddBox("FIing", 
    function(text, enter) 
        pcall(function()
            local target = findplr(text)
            if target then
                target.Character:FindFirstChild("ArmAngleChange"):FireServer(randomCFrame())
                LoopFlingV1[target.Name] = target
            end
        end)
    end, 
    "FIings Target")

local x2 = Main:AddBox("FIingV2", 
    function(text, enter) 
        pcall(function()
            local target = findplr(text)
            if target then
                target.Character:FindFirstChild("ArmAngleChange"):FireServer(randomCFrame())
                LoopFlingV2[target.Name] = target
            end
        end)
    end, 
    "FIingsV2 Target")

local x3 = Main:AddBox("LoopFIing", 
    function(text, enter) 
        pcall(function()
            local target = findplr(text)
            if target then
                LoopFlingV1[target.Name] = target
            end
        end)
    end, 
    "LoopFIings Target")

local x4 = Main:AddBox("LoopFIingV2", 
    function(text, enter) 
        pcall(function()
            local target = findplr(text)
            if target then
                LoopFlingV2[target.Name] = target
            end
        end)
    end, 
    "LoopFIingsV2 Target")

local b1 = Main:AddButton("ClearLoop", 
    function() 
        LoopFlingV1 = {}
        LoopFlingV2 = {}
    end, 
    "Clears LoopFIings/FIings")

game:GetService("Players").PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function(character)
        if LoopFlingV1[player.Name] then
            task.spawn(function()
                repeat
                    wait(0.1)
                    player.Character:FindFirstChild("ArmAngleChange"):FireServer(randomCFrame())
                until not LoopFlingV1[player.Name] or not player.Character or not player.Character.Parent
            end)
        end
        if LoopFlingV2[player.Name] then
            task.spawn(function()
                repeat
                    wait(0.1)
                    player.Character.ArmAngleChange:FireServer(randomCFrame())
                until not LoopFlingV2[player.Name] or not player.Character or not player.Character.Parent
            end)
        end
    end)
end)

